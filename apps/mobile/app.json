{
  "expo": {
    "name": "Founders Tribe",
    "slug": "founders-tribe",
    "scheme": "appfounderstribe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.founderstribe.ai",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/splash-icon.png"
      },
      "package": "com.founderstribe.ai",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FAFAF9",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/BricolageGrotesque_400Regular.ttf",
            "./assets/fonts/BricolageGrotesque_600SemiBold.ttf",
            "./assets/fonts/BricolageGrotesque_700Bold.ttf",
            "./assets/fonts/Poppins_400Regular.ttf",
            "./assets/fonts/Poppins_500Medium.ttf",
            "./assets/fonts/Poppins_600SemiBold.ttf",
            "./assets/fonts/Poppins_700Bold.ttf"
          ]
        }
      ],
      "@livekit/react-native-expo-plugin"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}